{
    "compilerOptions":{
        "target": "es6",
        "rootDir": "./src",
        "allowJs": true,
        "checkJs": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "./dist/",
        "typeRoots": [
            "node_modules/@types"
        ]
    },
    "include": [
       "./src/*.ts", "src/App.ts", "src/structures/Tree.ts", "src/structures/Nodo.ts",
    ],
    "exclude": [
       "node_modules",
    ]
}